  "version": "0.2.0",
  "configurations": [
    {
      "name": "Create Dictionary",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "args": ["c", "examples/example1.xml"]
    },
    {
      "name": "Dump Dictionary",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "args": ["d", "examples/example1.odict", "examples/output.xml"]
    }
  ]
}
