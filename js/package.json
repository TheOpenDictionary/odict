{
  "name": "theopendictionary",
  "version": "0.0.3",
  "description": "A JavaScript wrapper around the Open Dictionary compiler",
  "type": "module",
  "main": "dist/index.js",
  "repository": "https://github.com/TheOpenDictionary/odict",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "postpublish": "rimraf dist && git push --follow-tags",
    "build": "concurrently \"swc -d dist ./src/*.ts\" \"tsc\"",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.246",
    "@swc/jest": "^0.2.22",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  }
}
