{
  "name": "theopendictionary",
  "version": "0.0.5",
  "description": "A JavaScript wrapper around the Open Dictionary compiler",
  "type": "module",
  "main": "dist/index.js",
  "repository": "https://github.com/TheOpenDictionary/odict",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "postpublish": "rimraf dist && git push --follow-tags",
    "build": "concurrently \"swc -d dist ./src/*.ts\" \"tsc\"",
    "test": "tsc && jest"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "rimraf": "^4.0.0",
    "typescript": "^4.9.4"
  }
}
