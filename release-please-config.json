{
  "plugins": [
    {
      "type": "sentence-case"
    },
    "cargo-workspace"
  ],
  "bump-path-for-minor-pre-major": true,
  "include-component-in-tag": true,
  "packages": {
    "./cli": {
      "release-type": "rust",
      "component": "cli"
    },
    "./lib": {
      "release-type": "rust"
    },
    "./bindings/node": {
      "release-type": "node",
      "component": "node"
    },
    "./bindings/java": {
      "release-type": "java",
      "component": "java"
    },
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "rust",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}