{
  "name": "@odict/node",
  "version": "1.1.0",
  "type": "commonjs",
  "main": "index.js",
  "types": "index.d.ts",
  "browser": "browser.js",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "napi": {
    "binaryName": "node",
    "targets": [
      "universal-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-gnu",
      "riscv64gc-unknown-linux-gnu",
      "s390x-unknown-linux-gnu",
      "powerpc64le-unknown-linux-gnu",
      "i686-pc-windows-msvc",
      "armv7-unknown-linux-gnueabihf",
      "aarch64-linux-android",
      "armv7-unknown-linux-musleabihf",
      "x86_64-unknown-freebsd",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "armv7-linux-androideabi",
      "wasm32-wasip1-threads"
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheOpenDictionary/odict.git"
  },
  "devDependencies": {
    "@napi-rs/cli": "3.0.0-alpha.76",
    "@napi-rs/wasm-runtime": "^0.2.7",
    "@types/node": "^22.0.0",
    "buffer": "^6.0.3",
    "prettier": "^3.3.2",
    "vite": "^6.2.2",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^3.0.9"
  },
  "engines": {
    "node": ">= 10.16.0 < 11 || >= 11.8.0 < 12 || >= 12.0.0"
  },
  "scripts": {
    "create-dirs": "napi create-npm-dirs",
    "build": "napi build --release --platform --strip --pipe \"prettier -w\"",
    "build:wasm": "pnpm run build --target wasm32-wasip1-threads --no-default-features --features tokenize,json,sql",
    "build:debug": "napi build --pipe \"prettier -w\"",
    "dev:wasm": "vite dev",
    "test": "vitest --no-watch",
    "universalize": "napi universalize",
    "artifacts": "napi artifacts",
    "version": "napi version",
    "prepublishOnly": "napi prepublish --no-gh-release"
  },
  "optionalDependencies": {
    "@odict/node-darwin-universal": "0.0.3",
    "@odict/node-linux-x64-gnu": "0.0.3",
    "@odict/node-win32-x64-msvc": "0.0.3",
    "@odict/node-linux-x64-musl": "0.0.3",
    "@odict/node-linux-arm64-gnu": "0.0.3",
    "@odict/node-linux-riscv64-gnu": "0.0.3",
    "@odict/node-linux-s390x-gnu": "0.0.3",
    "@odict/node-linux-ppc64-gnu": "0.0.3",
    "@odict/node-win32-ia32-msvc": "0.0.3",
    "@odict/node-linux-arm-gnueabihf": "0.0.3",
    "@odict/node-android-arm64": "0.0.3",
    "@odict/node-linux-arm-musleabihf": "0.0.3",
    "@odict/node-freebsd-x64": "0.0.3",
    "@odict/node-linux-arm64-musl": "0.0.3",
    "@odict/node-win32-arm64-msvc": "0.0.3",
    "@odict/node-android-arm-eabi": "1.0.0",
    "@odict/node-wasm32-wasi": "0.0.3"
  }
}