enum ODictMethod:byte {
  Lookup = 1,
  Split = 2,
  Index = 3,
  Search = 4,
  Compile = 5,
  Write = 6,
  Lexicon = 7
}


table LookupPayload {
  id:string;
  follow:bool;
  split:int;
  queries:[string];
}

table WritePayload {
  id:string;
  xml:string;
  out:string;
}

table SplitPayload {
  id:string;
  threshold:int;
  query:string;
}

table SearchPayload {
  id:string;
  force:bool;
  exact:bool;
  query:string;
}

table CompilePayload {
  id:string;
  path:string;
  out:string;
}


